tb_FPU_unit:/tb_FPU_unit/dut/EXP_ADJUST_UNIT/i_exp_value \
tb_FPU_unit:/tb_FPU_unit/dut/EXP_ADJUST_UNIT/o_exp_result \
tb_FPU_unit:/tb_FPU_unit/dut/EXP_ADJUST_UNIT/w_exp_result \
tb_FPU_unit:/tb_FPU_unit/dut/EXP_ADJUST_UNIT/w_underflow_exp \
tb_FPU_unit:/tb_FPU_unit/dut/EXP_ADJUST_UNIT/w_overflow_exp \
tb_FPU_unit:/tb_FPU_unit/dut/EXP_ADJUST_UNIT/w_lopd_value
force -freeze tb_FPU_unit:/tb_FPU_unit/dut/EXP_SUB_UNIT/i_data_b {} 0
